{
	"name": "david63/acpversioncheck",
	"type": "phpbb-extension",
	"description": "Displays the status of the board phpBB versions.",
	"version": "2.1.0",
	"time": "2019-05-11",
	"keywords": [
		"ACP",
		"version",
		"check"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=5.4.0",
		"phpbb/phpbb": ">=3.2.0",
        "composer/installers": "~1.0"
    },
	"extra": {
        "display-name": "[+] ACP version check",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/acp_versioncheck_version_file.json"
    	}
	}
}

{
	"name": "david63/acpversioncheck",
	"type": "phpbb-extension",
	"description": "Displays the status of the board phpBB versions.",
	"version": "3.1.0-RC1",
	"time": "2019-12-27",
	"keywords": [
		"ACP",
		"version",
		"check"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.0",
		"phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0"
    },
	"extra": {
        "display-name": "[**] ACP version check",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "acp_versioncheck_version_file.json",
			"ssl": false
    	}
	}
}
